<% include ../partials/header %>
<div class="container">
<div class="row">
    <h1 style="text-align:center;">Create New CampGrounds</h1>
    <div style="width:30%;margin:50px auto;">
        <form action = "/campgrounds" method="POST">
         <div class="form-group">
             <input class="form-control" type="text" name="campname" placeholder="Camp Name">
         </div>
         <div class="form-group">
                <input class="form-control" type="text" name="image" placeholder="Image Url">
            </div>
            <div class="form-group">
                <input class="form-control" type="text" name="description" placeholder="Describe">
            </div>
         <div class="form-group">
             <button class="btn btn-lg btn-default btn-block btn-primary">Submit!</button>
            </div>
    
             </form>
             <a href="/campgrounds">Back</a>
    </div>




</div>
</div>
<% include ../partials/footer %>